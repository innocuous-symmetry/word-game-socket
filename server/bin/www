const { debug } = require('console');
const Game = require('../Game');
const socketio = require('socket.io');
const app = require('../.')

const http = require('http');
const server = http.createServer(app);

var io = socketio(server,{
  cors: {
    origin: '*',
    methods: ['GET', 'POST']
  }
});

Game(io);

server.listen(8080);
server.on('error', (err) => {
  throw new Error(err)
})

server.on('listening', () => {
  console.log("Listening on port 8080");
});
